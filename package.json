{
    "name": "@oasis-end/ui",
    "version": "0.0.2",
    "scripts": {
        "dev": "svelte-kit dev --port 3333",
        "build": "svelte-kit build",
        "package": "svelte-kit package & node --loader ts-node/esm scripts/build-css.ts",
        "preview": "svelte-kit preview",
        "prepare": "svelte-kit sync",
        "test": "playwright test",
        "check": "svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "prettier --check --plugin-search-dir=. . && eslint .",
        "format": "prettier --write --plugin-search-dir=. .",
        "deploy": "svelte-kit build && node --loader ts-node/esm scripts/gh-pages.ts",
        "release": "svelte-kit package & node --loader ts-node/esm scripts/build-css.ts && release-it"
    },
    "devDependencies": {
        "@playwright/test": "^1.21.0",
        "@sveltejs/adapter-auto": "next",
        "@sveltejs/adapter-static": "^1.0.0-next.32",
        "@sveltejs/kit": "next",
        "@types/gh-pages": "^3.2.1",
        "@types/prismjs": "^1.26.0",
        "@typescript-eslint/eslint-plugin": "^5.10.1",
        "@typescript-eslint/parser": "^5.10.1",
        "eslint": "^8.12.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-svelte3": "^4.0.0",
        "gh-pages": "^4.0.0",
        "mdsvex": "^0.10.5",
        "prettier": "^2.5.1",
        "prettier-plugin-svelte": "^2.5.0",
        "release-it": "^15.0.0",
        "sass": "^1.52.1",
        "svelte": "^3.44.0",
        "svelte-check": "^2.2.6",
        "svelte-preprocess": "^4.10.1",
        "svelte2tsx": "^0.5.10",
        "ts-node": "^10.8.0",
        "tslib": "^2.3.1",
        "typescript": "^4.7.2"
    },
    "type": "module",
    "dependencies": {
        "prism-svelte": "^0.5.0",
        "prismjs": "^1.28.0",
        "star-markdown-css": "^0.3.3"
    }
}
